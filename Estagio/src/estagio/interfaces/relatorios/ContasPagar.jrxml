	<queryString language="SQL">
		<![CDATA[select comp_nota_fiscal as nome, cast(parc_numero as varchar), parc_datavencimento, to_char(parc_valorparcela, 'L9G999G990D99') from parcela left join pagamento on parcela.parc_codigo = pagamento.parc_codigo 
	inner join compra on parcela.comp_codigo = parcela.comp_codigo 
		where parcela.comp_codigo > 0 and pag_codigo is null and parc_datavencimento >= '2021-11-08' and 
		parc_datavencimento <= '2021-11-13'
union 
